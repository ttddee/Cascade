QWidget {
    background-color: #181b1e;
    color: #b9b9b9;
    font: 12px "Open Sans";
    border: 0px;
    margin: 0px;
    padding: 0px;
}
QWidget#NodeBase {
    background-color: rgba(0, 0, 0, 0);
}
Cascade--Properties--PropertiesWindow {
    background-color: #282d31;
    margin: 0px;
}
Cascade--VulkanView {
    background-color: #181b1e;
}
Cascade--NodeGraph {
    margin: 0px;
    border: 0px;
    background-color: #282d31;
}
Cascade--ViewerStatusBar {
    background-color: #282d31;
}
QListView {
    background-color: #1d2024;
    font: 12px "Open Sans";
    color: #b9b9b9;
    border: none;
}
QListView::item:selected {
    background-color: #254a73;
    color: #b9b9b9;
}

QPlainTextEdit {
    background-color: #282d31;
    color: #b9b9b9;
}

/* Horizontal and Vertical Lines */
QFrame[frameShape="4"],
QFrame[frameShape="5"] {
    background-color:#626971;
    border: none;
    max-height: 1px;
}

/* Tabs in Color Node*/
QWidget#master {
    background-color: #282d31;
}
QWidget#shadows {
    background-color: #282d31;
}
QWidget#midtones {
    background-color: #282d31;
}
QWidget#highlights {
    background-color: #282d31;
}

QPushButton {
    background-color: #1d2024;
    color: #b9b9b9;
    border: none;
    border-radius: 2px;
    font-weight: bold;
}
QPushButton:hover {
    background-color: #626971;
}
QPushButton:pressed {
    background-color: #254a73;
}
QPushButton:disabled {
    color: #9299a1;
    background-color: #1d2024;
}

QScrollBar:vertical {
    border: 0px;
    width: 15px;
    background: #1d2024;
}
QScrollBar::handle:vertical {
    background: #626971;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     border: 0px;
     background: none;
     color: none;
}
QScrollBar::add-line:vertical {
      border: none;
      background: none;
}
QScrollBar::sub-line:vertical {
      border: none;
      background: none;
}

QScrollBar:horizontal {
    border: 0px;
    height: 15px;
    background: #1d2024;
}
QScrollBar::handle:horizontal {
    background: #626971;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
     border: 0px;
     background: none;
     color: none;
 }
 QScrollBar::add-line:horizontal {
       border: none;
       background: none;
 }
 QScrollBar::sub-line:horizontal {
       border: none;
       background: none;
 }

